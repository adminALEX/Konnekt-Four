<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="gridpane" style="-fx-background-color: #d3a5e8;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.connect4game.HelloController">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints maxWidth="296.0" minWidth="191.0" prefWidth="204.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="30.0" minHeight="10.0" />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane fx:id="menupane" GridPane.columnSpan="2" />
        <Pane fx:id="playpane" prefHeight="400.0" prefWidth="200.0" GridPane.rowIndex="1" />
      <VBox fx:id="contentpane" style="-fx-background-color: #461e5c;" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <TextField fx:id="p1name" promptText="Enter Player1 Name">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin></TextField>
            <TextField fx:id="p2name" promptText="Enter Player2 Name">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin></TextField>
            <Button fx:id="setName" mnemonicParsing="false" prefHeight="25.0" prefWidth="209.0" text="SetName">
               <VBox.margin>
                  <Insets left="50.0" right="50.0" top="10.0" />
               </VBox.margin></Button>
            <Region prefHeight="39.0" prefWidth="204.0" VBox.vgrow="ALWAYS" />
            <Label fx:id="playername" alignment="CENTER" prefHeight="39.0" prefWidth="302.0" text="PLAYER ONE" textAlignment="JUSTIFY" textFill="WHITE">
               <font>
                  <Font name="JetBrains Mono ExtraBold" size="32.0" />
               </font></Label>
            <Label alignment="CENTER" prefHeight="32.0" prefWidth="303.0" text="TURN" textAlignment="CENTER" textFill="#fffbfb">
               <font>
                  <Font name="JetBrains Mono Regular" size="24.0" />
               </font>
            </Label>
            <Region prefHeight="122.0" prefWidth="204.0" VBox.vgrow="ALWAYS" />
         </children></VBox>
    </children>
</GridPane>
